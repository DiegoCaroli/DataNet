<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.android.diego.datanet"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".NetActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".NodeListActivity"
            android:label="@string/title_activity_node_list"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
            android:name=".NewNodeActivity"
            android:label="@string/title_activity_new_node">
        </activity>
        <activity android:name=".CreateNetFromFileActivity">
        </activity>
    </application>

</manifest>
